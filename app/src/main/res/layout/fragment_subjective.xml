<?xml version="1.0" encoding="utf-8"?>
<GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="horizontal"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
    android:columnCount="3"
    android:rowCount="5"
    android:background="#ffbbbbbb">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/cc"
        android:id="@+id/subjective_cc"
        android:layout_row="0"
        android:layout_column="0" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/pmh"
        android:id="@+id/subjective_pmh"
        android:layout_row="0"
        android:layout_column="1" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/fmh"
        android:id="@+id/subjective_fmh"
        android:layout_row="0"
        android:layout_column="2" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="300dp"
        android:inputType="textMultiLine"
        android:ems="10"
        android:id="@+id/editText9"
        android:layout_row="1"
        android:layout_column="0"
        android:background="#ffffffff" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="300dp"
        android:inputType="textMultiLine"
        android:ems="10"
        android:id="@+id/editText10"
        android:layout_row="1"
        android:layout_column="1"
        android:background="#ffffffff" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="300dp"
        android:inputType="textMultiLine"
        android:ems="10"
        android:id="@+id/editText11"
        android:layout_row="1"
        android:layout_column="2"
        android:background="#ffffffff" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/sh"
        android:id="@+id/subjective_sh"
        android:layout_row="2"
        android:layout_column="0" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/meds"
        android:id="@+id/subjective_meds"
        android:layout_row="2"
        android:layout_column="1" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/ros"
        android:id="@+id/subjective_ros"
        android:layout_row="2"
        android:layout_column="2" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="300dp"
        android:inputType="textMultiLine"
        android:ems="10"
        android:id="@+id/editText12"
        android:layout_row="3"
        android:layout_column="0"
        android:background="#ffffffff" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="300dp"
        android:inputType="textMultiLine"
        android:ems="10"
        android:id="@+id/editText13"
        android:layout_row="3"
        android:layout_column="1"
        android:background="#ffffffff" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="300dp"
        android:inputType="textMultiLine"
        android:ems="10"
        android:id="@+id/editText14"
        android:layout_row="3"
        android:layout_column="2"
        android:background="#ffffffff" />

</GridLayout>